                L(i,j)=0;
                U(i,j)=-A(i,j);
                end
          j=j+1;
      end
      i=i+1;
end

i=0;
j=0;
m=norm(x-x0,'inf');
while m>5e-6
    n=L*x;
    n=n(:);
    j=1;
    while j<=long
        if j==1
           x(j)=(1-omega)*x(j)+omega*((b(j)-A(1,j+1:long)*x(j+1:long))/A(j,j)); 
          
        elseif j>1&&j<long
            x(j)=(1-omega)*x(j)+omega*((b(j)-A(j,j+1:long)*x(j+1:long)-A(j,1:j-1)*x(1:j-1))/A(j,j));
            
        elseif j==long
