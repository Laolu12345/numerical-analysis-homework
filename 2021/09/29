      j=1;
      while j<=long  
             if i-j==0 
             D(i,j)=A(i,j);
             L(i,j)=0;
             U(i,j)=0;
             end
             if i-j>0
                 D(i,j)=0;
                 L(i,j)=-A(i,j);
                 U(i,j)=0;
                end
