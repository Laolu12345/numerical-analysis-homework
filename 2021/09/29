//P210-7)-(2）

//omega=1.03

clear;
clc;
A=[4,-1,0;-1,4,-1;0,-1,4];
b=[1;4;-3];
x=[0;0;0];
x0=[0.5;1;-0.5];
omega=1.03;
long=size(A,1);
i=1;
while i<=long
      j=1;
      while j<=long  
             if i-j==0 
             D(i,j)=A(i,j);
             L(i,j)=0;
             U(i,j)=0;
             end
             if i-j>0
                 D(i,j)=0;
                 L(i,j)=-A(i,j);
                 U(i,j)=0;
                end
            if i-j<0
                D(i,j)=0;
                L(i,j)=0;
                U(i,j)=-A(i,j);
                end
          j=j+1;
      end
      i=i+1;
end

i=0;
j=0;
m=norm(x-x0,'inf');
while m>5e-6
    n=L*x;
    n=n(:);
    j=1;
    while j<=long
        if j==1
           x(j)=(1-omega)*x(j)+omega*((b(j)-A(1,j+1:long)*x(j+1:long))/A(j,j)); 
          
        elseif j>1&&j<long
            x(j)=(1-omega)*x(j)+omega*((b(j)-A(j,j+1:long)*x(j+1:long)-A(j,1:j-1)*x(1:j-1))/A(j,j));
            
        elseif j==long
            x(long)=(1-omega)*x(j)+omega*((b(long)-A(long,1:long-1)*x(1:long-1))/A(long,long));
      
        end
        j=j+1;
    end
    x=x(:);
    i=i+1;
    m=abs(norm(x-x0,'inf'));
end

//omega=1.03时，迭代次数为5

//omega=1

clear;
clc;
