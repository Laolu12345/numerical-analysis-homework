                 D(i,j)=0
                 L(i,j)=-A(i,j)
                 U(i,j)=0
                end
            if i-j<0
                D(i,j)=0
                L(i,j)=0
                U(i,j)=-A(i,j)
                end
          j=j+1
      end
      i=i+1
end
B=inv(D)*(L+U);
[Alpha,Lambda]=eig(B);
Lambda=diag(Lambda);
LambdaMAX=max(abs(Lambda));

LambdaMAX为1.09282032302755>1

